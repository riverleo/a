{
  "name": "a",
  "version": "1.0.0",
  "description": "Workslow Web Application",
  "main": "index.js",
  "repository": "git@github.com:workslow/a.git",
  "author": "riverleo <iam.riverleo@gmail.com>",
  "license": "UNLICENSE",
  "private": true,
  "scripts": {
    "all": "concurrently --kill-others -n 'NEXT,TEST,LINT' -c 'cyan.bold' 'npm run dev' 'npm run test:watch' 'npm run lint:watch'",
    "dev": "node ./scripts/server.js",
    "build": "next build",
    "start": "next build && NODE_ENV=production node ./scripts/server.js",
    "deploy": "pm2 deploy ecosystem.config.js a",
    "lint": "esw --color",
    "lint:watch": "esw --watch --color",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test:generate-output -- --coverage",
    "test:generate-output": "jest --json --outputFile=jest-test-results.json --silent",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "storybook:deploy": "npm run storybook:build && node ./scripts/uploadStorybookToS3.js"
  },
  "jest": {
    "watchPathIgnorePatterns": [
      "jest-test-results.json"
    ],
    "setupFiles": [
      "<rootDir>/lib/initJest.js"
    ]
  },
  "dependencies": {
    "@storybook/addon-jest": "^3.4.6",
    "@storybook/react": "^3.4.6",
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "classnames": "^2.2.5",
    "codemirror": "^5.38.0",
    "cookie-parser": "^1.4.3",
    "enzyme": "^3.3.0",
    "express": "^4.16.3",
    "form-data": "^2.3.2",
    "glob": "^7.1.2",
    "immutable": "^3.8.2",
    "js-cookie": "^2.1.4",
    "lodash": "^4.17.5",
    "next": "^6.0.3",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-codemirror": "^1.0.0",
    "react-dnd": "^3.0.2",
    "react-dnd-html5-backend": "^4.0.4",
    "react-dom": "^16.4.0",
    "react-intl": "^2.3.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-observable": "^0.18.0",
    "rxjs": "^6.2.0",
    "rxjs-compat": "^6.2.0",
    "styled-components": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.42",
    "@babel/preset-env": "7.0.0-beta.42",
    "@babel/preset-stage-0": "7.0.0-beta.42",
    "@storybook/addon-viewport": "^3.4.6",
    "aws-sdk": "^2.249.1",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-styled-components": "^1.5.1",
    "concurrently": "^3.5.1",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.8.2",
    "eslint-watch": "^3.1.5",
    "jest": "^23.1.0",
    "mime-types": "^2.1.18",
    "pm2": "^2.10.4",
    "webpack": "^4.10.2"
  }
}
